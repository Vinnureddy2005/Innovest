{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": ["./types", "./node_modules/@types"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/SessionWrapper.tsx", "app/components/SessionWrapper.tsx", "app/api/auth/[...nextauth]/route.js", "app/api/auth/[...nextauth]/route.js", "app/components/works.js", "app/components/contact.js", "app/client_dashboard/DashboardContent.js", "app/investor_dashboard/page.js", "app/investor_dashboard/DashboardContent.js"],
  "exclude": ["node_modules"]
}
